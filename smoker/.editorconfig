[*]
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
charset = utf-8

indent_style = tab
max_line_length = 120

[*.org]
trim_trailing_whitespace = true
max_line_length = off
indent_style = space

[*.{md,markdown}]
max_line_length = off
indent_style = space
indent_size = 4

